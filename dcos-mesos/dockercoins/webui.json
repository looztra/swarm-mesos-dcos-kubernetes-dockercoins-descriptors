{
  "id": "webui",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "looztra/dockercoins_webui:1.1",
      "network": "BRIDGE",
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 80,
          "servicePort": 10000
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "DOCKERCOINS_REDIS_HOST": "marathon-lb-internal.marathon.mesos",
    "DOCKERCOINS_REDIS_PORT": "10001"
  },
  "instances": 1,
  "cpus": 0.5,
  "mem": 500,
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "portIndex": 0,
      "timeoutSeconds": 10,
      "gracePeriodSeconds": 10,
      "intervalSeconds": 2,
      "maxConsecutiveFailures": 10
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "cfk-midiconf-dcosagents.westeurope.cloudapp.azure.com",
    "HAPROXY_0_MODE": "http"
  }
}
