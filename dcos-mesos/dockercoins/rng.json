{
  "id": "rng",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "looztra/dockercoinsextract_rng:1.0",
      "network": "BRIDGE",
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 80,
          "servicePort": 10002
        }
      ],
      "forcePullImage": true
    }
  },
  "instances": 1,
  "cpus": 0.5,
  "mem": 250,
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "portIndex": 0,
      "timeoutSeconds": 10,
      "gracePeriodSeconds": 10,
      "intervalSeconds": 2,
      "maxConsecutiveFailures": 10
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "internal"
  }
}
