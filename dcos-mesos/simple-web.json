{
  "id": "web",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "yeasy/simple-web",
      "network": "BRIDGE",
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 80,
          "servicePort": 10000
        }
      ],
      "forcePullImage": true
    }
  },
  "env": {
    "DUMMY_ENV_VAR": "ZENIKA RULEZ"
  },
  "instances": 3,
  "cpus": 0.1,
  "mem": 65,
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "portIndex": 0,
      "timeoutSeconds": 10,
      "gracePeriodSeconds": 10,
      "intervalSeconds": 2,
      "maxConsecutiveFailures": 10
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "cfk-midiconf-dcosagents.westeurope.cloudapp.azure.com",
    "HAPROXY_0_MODE": "http"
  }
}
